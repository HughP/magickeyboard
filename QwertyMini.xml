<layout name="Mini QWERTY Keyboard" definition="Def_iOSMain">
	<keys>
		<key button="1"  letter="q"         keycode="12" />
		<key button="2"  letter="w"         keycode="13" />
		<key button="3"  letter="e"         keycode="14" />
		<key button="4"  letter="r"         keycode="15" />
		<key button="5"  letter="t"         keycode="17" />
		<key button="6"  letter="y"         keycode="16" />
		<key button="7"  letter="u"         keycode="32" />
		<key button="8"  letter="i"         keycode="34" />
		<key button="9"  letter="o"         keycode="31" />
		<key button="10" letter="p"         keycode="35" />
		<key button="11" letter="a"         keycode="0"  />
		<key button="12" letter="s"         keycode="1"  />
		<key button="13" letter="d"         keycode="2"  />
		<key button="14" letter="f"         keycode="3"  />
		<key button="15" letter="g"         keycode="5"  />
		<key button="16" letter="h"         keycode="4"  />
		<key button="17" letter="j"         keycode="38" />
		<key button="18" letter="k"         keycode="40" />
		<key button="19" letter="l"         keycode="37" />
		<key button="20" letter="SHIFT"     keycode="56" />
		<key button="21" letter="z"         keycode="6"  />
		<key button="22" letter="x"         keycode="7"  />
		<key button="23" letter="c"         keycode="8"  />
		<key button="24" letter="v"         keycode="9"  />
		<key button="25" letter="b"         keycode="11" />
		<key button="26" letter="n"         keycode="45" />
		<key button="27" letter="m"         keycode="46" />
		<key button="28" letter="DELETE"    keycode="51" />
		<key button="29" letter="NUMS"      keycode="0"  />
		<key button="30" letter="MODIFIERS" keycode="0"  />
		<key button="31" letter=" "         keycode="49" />
		<key button="32" letter="RETURN"    keycode="36" />
	</keys>
</layout>
