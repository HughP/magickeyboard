<layout name="Mini Numbers Keyboard" definition="Def_iOSNums">
	<keys>
		<key button="1"  letter="1"      keycode="18"  />
		<key button="2"  letter="2"      keycode="19"  />
		<key button="3"  letter="3"      keycode="20"  />
		<key button="4"  letter="4"      keycode="21"  />
		<key button="5"  letter="5"      keycode="23"  />
		<key button="6"  letter="6"      keycode="22"  />
		<key button="7"  letter="7"      keycode="26"  />
		<key button="8"  letter="8"      keycode="28"  />
		<key button="9"  letter="9"      keycode="25"  />
		<key button="10" letter="0"      keycode="29"  />
		<key button="11" letter="-"      keycode="27"  />
		<key button="12" letter="/"      keycode="44"  />
		<key button="13" letter=":"      keycode="S41" />
		<key button="14" letter=";"      keycode="41"  />
		<key button="15" letter="("      keycode="S25" />
		<key button="16" letter=")"      keycode="S29" />
