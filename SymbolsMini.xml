<layout name="Mini Symbols Keyboard" definition="Def_iOSSym">
	<keys>
		<key button="1"  letter="["      keycode="33"  />
		<key button="2"  letter="]"      keycode="30"  />
		<key button="3"  letter="{"      keycode="S33" />
		<key button="4"  letter="}"      keycode="S30" />
		<key button="5"  letter="#"      keycode="S20" />
		<key button="6"  letter="%"      keycode="S23" />
		<key button="7"  letter="^"      keycode="S22" />
		<key button="8"  letter="*"      keycode="S28" />
		<key button="9"  letter="+"      keycode="S24" />
		<key button="10" letter="="      keycode="24"  />
		<key button="11" letter="_"      keycode="S27" />
		<key button="12" letter="\"      keycode="42"  />
		<key button="13" letter="|"      keycode="S42" />
		<key button="14" letter="~"      keycode="S50" />
		<key button="15" letter="&lt;"   keycode="S43" />
		<key button="16" letter="&gt;"   keycode="S47" />
		<key button="17" letter="€"      keycode="0"   /> <!-- Missing -->
		<key button="18" letter="£"      keycode="0"   /> <!-- Missing -->
		<key button="19" letter="¥"      keycode="0"   /> <!-- Missing -->
		<key button="20" letter="•"      keycode="0"   /> <!-- Missing -->
		<key button="21" letter="NUMS"   keycode="0"   />
		<key button="22" letter="."      keycode="47"  />
		<key button="23" letter=","      keycode="43"  />
		<key button="24" letter="?"      keycode="S44" />
		<key button="25" letter="!"      keycode="S18" />
		<key button="26" letter="&apos;" keycode="39"  />
		<key button="27" letter="DELETE" keycode="51"  />
		<key button="28" letter="QWERTY" keycode="0"   />
		<key button="29" letter=" "      keycode="49"  />
		<key button="30" letter="RETURN" keycode="36"  />
	</keys>
</layout>
